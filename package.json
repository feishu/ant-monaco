{
  "name": "ant-monaco",
  "version": "0.3.1",
  "description": "Ant monaco with vscode-textmate. This module can only run under node runtime",
  "main": "lib/min/index.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack -w",
    "minify": "uglifyjs lib/index.js -o lib/index.min.js && npm run clean",
    "clean": "node scripts/clean.js",
    "js": "node scripts/javascript.js && npm run js-extra",
    "js-extra": "node scripts/js-extra.js",
    "api": "node scripts/completions.js $site"
  },
  "keywords": [
    "monao",
    "vscode-textmate",
    "ant"
  ],
  "author": "munong",
  "license": "MIT",
  "dependencies": {
    "eslint": "^3.19.0",
    "vscode-textmate": "^3.1.4"
  },
  "devDependencies": {
    "@types/node": "^7.0.22",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "csslint": "^1.0.5",
    "fs-extra": "^3.0.1",
    "jshint": "^2.9.4",
    "mark-twain": "^1.1.6",
    "node-ipc": "^9.0.1",
    "request": "^2.81.0",
    "typescript": "^2.3.3",
    "uglify-es": "^3.0.13",
    "vscode": "^1.1.0",
    "webpack": "^2.6.1"
  }
}
